package org.elusive.sound.blocs.frequenceur;


public class NotePlacee {

	private int debut = 0;
	private double frequence = 440;
	private int duree = 44100;

	public NotePlacee(int debut, double frequence, int duree) {
		this.debut = debut;
		this.frequence = frequence;
		this.duree = duree;
	}

	public int getDebut() {
		return debut;
	}

	public void setDebut(int debut) {
		this.debut = debut;
	}

	public double getFrequence() {
		return frequence;
	}

	public void setFrequence(double frequence) {
		this.frequence = frequence;
	}

	public int getDuree() {
		return duree;
	}

	public void setDuree(int duree) {
		this.duree = duree;
	}
	
	public String toString(){
		StringBuffer sb = new StringBuffer("NotePlacee[");
		sb.append(" début = "+debut);
		sb.append(", frequence = "+frequence);
		sb.append(", durée = "+duree);
		sb.append(']');
		return sb.toString();
	}


}
